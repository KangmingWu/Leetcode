        "def maxArea(height) -\u003e int:\n",
        "    l, r \u003d 0, len(height)-1\n",
        "    ans \u003d 0\n",
        "    while l\u003cr:\n",
        "        ans \u003d max(ans,min(height[l], height[r])*(r-l))\n",
        "        if height[l]\u003eheight[r]: r-\u003d1\n",
        "        else: l+\u003d1\n",
        "    return ans"
      ]
    },
